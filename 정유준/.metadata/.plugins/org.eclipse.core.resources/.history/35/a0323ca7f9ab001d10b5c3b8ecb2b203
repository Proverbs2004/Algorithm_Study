package day0213;

import java.util.Arrays;
import java.util.Scanner;

public class BigNum {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int testCase = sc.nextInt();
		
		for(int i = 0; i < testCase; i++) {
			int N = sc.nextInt();
			int[] arr = new int[N];
			
			Arrays.sort(arr);
			System.out.println("#" + testCase + " " + arr[i]);
		}
		sc.close();
	}
}

