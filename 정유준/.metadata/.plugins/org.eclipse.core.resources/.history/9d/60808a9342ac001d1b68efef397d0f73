import java.util.Scanner;

public class B2798 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int cardNum = sc.nextInt();
		int dealerNum = sc.nextInt();
		int[] cardArr= new int[cardNum];
		
		//카드값 입력받기
		for(int idx = 0; idx < cardNum; idx++) {
			cardArr[idx] = sc.nextInt();
		}
		
		int min = 300000;
		//for문 3개돌리면서 최소차이 찾기 ^^
		for(int i = 0; i < cardNum-2; i++) {
			for(int j = i+1; j < cardNum-1; j++) {
				for(int z = j+1; z < cardNum; z++) {
					if(dealerNum - cardArr[i] - cardArr[j] - cardArr[z] >= 0 && dealerNum - cardArr[i] - cardArr[j] - cardArr[z] <= min) {
						
					}
				}
			}
		}
		System.out.println(min);
		
		
	}
}
