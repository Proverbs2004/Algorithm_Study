package day0220;

import java.util.Scanner;

public class SWEA_5356_의석이의세로로말해요_정유준 {
	static Scanner sc = new Scanner(System.in);
	static char[][] words;
	static int wordSize;

	static void input() {
		wordSize = sc.nextInt();
		words = new char[wordSize][wordSize];
	}

	static void print() {
		for (int col = 0; col < wordSize - 1; col++) {
			for (int row = 0; row < wordSize - 1; row++) {
				System.out.print(words[col][row]);
			}
		}
	}

	static void solve() {
		input();
		print();
	}

	public static void main(String[] args) {

	}
}
