package day0215;

import java.util.Scanner;

public class Sum {

static Scanner sc= new Scanner(System.in);
static int arr[][] = new int[100][100];

static void input() {
	for(int col = 0; col < 100; col++) {
		for(int row = 0; row < 100; row++) {
			arr[col][row] = sc.nextInt();
		}
	}
}

static int sum() {
	int max = 0;
	//가로 합 비교
	for(int col = 0; col < 100; col++) {
		int tempMax = 0;
		for(int row = 0; row < 100; row++) {
			tempMax += arr[col][row];
		}
		if(tempMax > max) {
			max = tempMax;
		}
	}
	
	//세로 합 비교
	for(int row = 0; row < 100; row++) {
		int tempMax = 0;
		for(int col = 0; col < 100; col++) {
			tempMax += arr[col][row];
		}
		if(tempMax > max) {
			max = tempMax;
		}
	}
	
	//좌상우하 대각선
	
}
	public static void main(String[] args) {

	}
}
