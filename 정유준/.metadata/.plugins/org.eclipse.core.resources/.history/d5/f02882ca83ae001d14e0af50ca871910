package day0217;

import java.util.Scanner;

public class SWEA_1216_회문2_정유준 {

	static Scanner sc = new Scanner(System.in);

	static int maxCount;

	static char[][] charPan = new char[100][100];

	static String checkRow;
	static String checkCol;

	static void input() {
		int testCase = sc.nextInt();

		// 배열 값 채우기
		for (int i = 0; i < 100; i++) {
			charPan[i] = sc.next().toCharArray();
		}
	}

	static void makeColString(int row, int col) {
		checkCol = "";
		for (int idx = 0; idx < check; idx++) {
			if (row >= 8) {
				break;
			} else {
				checkCol += charPan[row][col];
				row++;
			}
		}
	}

	static void makeRowString(int row, int col) {
		checkRow = "";
		for (int idx = 0; idx < check; idx++) {
			if (col >= 8) {
				break;
			} else {
				checkRow += charPan[row][col];
				col++;
			}
		}
	}

	public static void main(String[] args) {

	}
}
